              <groupId>org.codehaus.groovy</groupId>
              <artifactId>groovy-eclipse-batch</artifactId>
              <version>1.8.6-01</version>
            </dependency>
          </dependencies>
          -->
      </plugin>

      <!-- eclipse插件 -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-eclipse-plugin</artifactId>
        <version>2.8</version>
        <configuration>
          <wtpversion>2.0</wtpversion>
          <sourceExcludes>
            <sourceExclude>**/.svn/</sourceExclude>
          </sourceExcludes>
          <downloadSources>true</downloadSources>
          <buildOutputDirectory>src/main/webapp/WEB-INF/classes</buildOutputDirectory>
        </configuration>
      </plugin>

      <!-- jetty插件 -->
      <plugin>
        <groupId>org.mortbay.jetty</groupId>
        <artifactId>jetty-maven-plugin</artifactId>
      </plugin>

      <!-- test插件,加大内存 -->
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <configuration>
          <argLine>-Xmx128M</argLine>
          <useSystemClassLoader>false</useSystemClassLoader>
        </configuration>
      </plugin>

      <!-- cobertura插件 ,取消不需要计算覆盖率的类 -->
      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>cobertura-maven-plugin</artifactId>
        <configuration>
          <instrumentation>
            <excludes>
              <exclude>**/entity/**/*.class</exclude>
            </excludes>
          </instrumentation>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>sonar-maven-plugin</artifactId>
        <version>2.0</version>
      </plugin>
    </plugins>

  </build>

  <profiles>
    <!-- yui compressor -->
    <profile>
      <id>yui-compressor-id</id>
      <!-- 激活条件-->
      <activation>
        <property>
          <name>yui</name>
          <value>true</value>
        </property>
      </activation>

      <build>
        <plugins>

          <!--
            mvn net.alchim31.maven:yuicompressor-maven-plugin:jslint
            mvn net.alchim31.maven:yuicompressor-maven-plugin:compress -Dmaven.yuicompressor.force=true -Dmaven.yuicompressor.failOnWarning=true -e
            <goal>jslint</goal>
            -->
          <plugin>
            <groupId>net.alchim31.maven</groupId>
            <artifactId>yuicompressor-maven-plugin</artifactId>
            <version>1.2</version>
            <executions>
              <execution>
                <phase>package</phase>
                <goals>
                  <goal>compress</goal>
                </goals>
              </execution>
            </executions>
            <configuration>
              <encoding>utf8</encoding>
              <nosuffix>true</nosuffix>
              <force>true</force>
              <failOnWarning>false</failOnWarning>
              <jswarn>false</jswarn>
              <excludes>
                <exclude>**/ext/**/**.js</exclude>
                <exclude>**/ext/**/**.css</exclude>
                <exclude>**/jquery/**.js</exclude>
                <exclude>**/js/ux/**.js</exclude>
                <exclude>**/blueprint/**/**.css</exclude>
              </excludes>
            </configuration>
          </plugin>
        </plugins>

      </build>
    </profile>

    <!-- add el lib -->
    <profile>
      <id>add-el-id</id>
      <!-- 激活条件-->
      <activation>
        <property>
          <name>add-el</name>
